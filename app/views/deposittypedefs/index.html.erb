<%= render 'layerbegin' %>
<div class="col-lg-12">
  <div class="ibox float-e-margins">
    <div class="ibox-title">
      <h3>存入方式管理</h3>
    </div>
    <div class="ibox-content">
      <table class="table table-hover table-bordered text-center">
        <thead>
        <tr>
          <th class="text-center">存入方式</th>
          <th class="text-center">操作</th>
        </tr>
        </thead>
        <tbody>
        <% @deposittypedefs.each do |deposittype| %>
            <tr>
              <td><%= deposittype.depostitype %></td>
              <td>
                <%= link_to '编辑',edit_deposittypedef_path(deposittype) %>
                <%= link_to '删除', deposittype, method: :delete, data: { confirm: '确认删除?' } %>

                    </td>
            </tr>
        <% end %>
        </tbody>
      </table>
      <%= link_to "新增",new_deposittypedef_path,class: "btn btn-w-m btn-primary pull-right" %>
      <br>
    </div>
  </div>
</div>
<%=render 'layerend' %>