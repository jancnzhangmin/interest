<%= render 'layerbegin' %>
<div class="col-lg-12">
  <div class="ibox float-e-margins">
    <div class="ibox-title">
      <h3>存息版本管理</h3>
    </div>
    <div class="ibox-content">
      <table class="table table-hover table-bordered text-center">
        <thead>
        <tr>
          <th class="text-center">名称</th>
          <th class="text-center">日息</th>
          <th class="text-center">操作</th>
        </tr>
        </thead>
        <tbody>
        <% @interestversions.each do |interestversion| %>
            <tr>
              <td><%= interestversion.name %></td>
              <td><%= interestversion.rate %></td>


              <td>
                <%= link_to '编辑',edit_interestversion_path(interestversion) %>
                <%= link_to '删除', interestversion, method: :delete, data: { confirm: '确认删除?' } %>

                    </td>
            </tr>
        <% end %>
        </tbody>
      </table>
      <%= link_to "新增",new_interestversion_path,class: "btn btn-w-m btn-primary pull-right" %>
      <br>
    </div>
  </div>
</div>
<%=render 'layerend' %>